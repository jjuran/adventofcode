#!/usr/bin/env vx

const bindir = dirname realpath argv[ 0 ]

const data = (load bindir "/12.txt").lines()[ 0 ]

const ev = eval data

var sum = 0

def descend
{
	if _ isa int then
	{
		sum += _
	}
	else if _ isa array then
	{
		for x in _ do
		{
			descend x
		}
	}
	else if _ isa function then
	{
		const table = str^[ _() ]
		
		const values = table.values
		
		if not "red" in (values ver {v isa string}) then
		{
			descend values
		}
	}
}

descend ev

print sum
