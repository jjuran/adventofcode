#!/usr/bin/env vx

const bindir = dirname realpath argv[ 0 ]

const input_path = if argc > 1 then {argv[ 1 ]} else {bindir "/input.txt"}

const data = (load input_path).lines()[ 0 ]

var p = begin data

var len = 0

while p do
{
	if *p++ == '(' then
	{
		var q = p.find ')'
		
		const marker = q - p
		
		const m_len, const m_rep = *([marker / 'x'] map int)
		
		len += m_len * m_rep
		
		p = ++q + m_len
	}
	else
	{
		++len
	}
}

print len
