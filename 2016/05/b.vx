#!/usr/bin/env vx

const data = "ugkcyxxp"

const n = 5

var suffix = 0

var password = " " * 8

while ' ' in password do
{
	if (hex md5 data ++suffix)[ 0 -> n ] == "0" * n then
	{
		const output = hex md5 data suffix
		
		warn rep (data suffix, output)
		
		const o6 = output[ 5 ]
		
		if o6 <= '7' then
		{
			const pos = u8 o6 - 0x30
			
			if password[ pos ] == ' ' then
			{
				password[ pos ] = output[ 6 ]
				
				print password
			}
		}
	}
}

print password
