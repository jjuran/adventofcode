#!/usr/bin/env vx

const data = "ugkcyxxp"

const n = 5

var suffix = 0

var password = ""

while password.length < 8 do
{
	if (hex md5 data ++suffix)[ 0 -> n ] == "0" * n then
	{
		const output = hex md5 data suffix
		
		warn rep (data suffix, output)
		
		password = password output[ 5 ]
	}
}

print password
