#!/usr/bin/env vx

const bindir = dirname realpath argv[ 0 ]

const input_path = if argc > 1 then {argv[ 1 ]} else {bindir "/input.txt"}

const data = (load input_path).lines() map int

for a in data do
{
	for b in data do
	{
		for c in data do
		{
			if a + b + c == 2020 then
			{
				print (a * b * c)
			}
		}
	}
}
