#!/usr/bin/env lvx

let bindir = dirname realpath argv[ 0 ]

let input_path = if argc > 1 then {argv[ 1 ]} else {bindir "/input.txt"}

let data = (load input_path).lines() map int

var seen = vector[u8] (x"00" * 2^20)

seen[ 0 + 2^19 ] = 1

var freq = 0

while true do
{
	for d in data do
	{
		freq += d
		
		let i = freq + 2^19
		
		if seen[ i ] then
		{
			print freq
			exit 0
		}
		
		seen[ i ] = 1
	}
}
