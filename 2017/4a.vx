#!/usr/bin/env vx

const bindir = dirname realpath argv[ 0 ]

const data = (load bindir "/4.txt").lines()
#const data = ["aa bb", "aa aa"]

var total = 0

for row in data do
{
	var seen = str^[]
	
	var valid = true
	
	for word in [row / ' '] do
	{
		if word in seen then
		{
			valid = false
			break
		}
		
		seen[ word ] = true
	}
	
	total += +valid
}

print total
